  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
    count(*) AS Total,
    sum(case when Winner = 0 then 1 else 0 end) AS XCount,
    sum(case when Winner = 1 then 1 else 0 end) AS OCount
FROM Game
where UserPlayerType = Winner
GROUP BY Winner</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
